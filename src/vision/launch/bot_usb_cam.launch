<launch>
	<arg name="camera" default="bot_usb_cam" />
	
    <node name="uvc_cam" pkg="uvc_camera" type="uvc_camera_node" output="screen" ns="bot_camera/camera">
   		<param name="camera_info_url" value="package://vision/launch/bot_usb_cam_calibration.yaml"/>
        <param name="device" value="/dev/video0" />
        <param name="width" value="640" />
        <param name="height" value="480" />
        <param name="fps" value="30" />
    </node>
   
</launch>
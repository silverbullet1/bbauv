<launch>
    <node pkg="nodelet" type="nodelet" name="imu_core" args="manager"
            output="screen" />
    <node pkg="nodelet" type="nodelet" name="GEDC6"
            args="load imu/IMU imu_core" output="screen">
        <param name="rate" value="20" />
        <param name="name" value="GEDC6" />
        <param name="port"  value="/dev/tty.usbserial-FTFUT713" />
    </node>
    <node pkg="nodelet" type="nodelet" name="AHRS8"
            args="load imu/IMU imu_core" output="screen">
        <param name="rate" value="20" />
        <param name="name" value="AHRS8" />
        <param name="port"  value="/dev/tty.usbserial-FTFUUY6A" />
    </node>
</launch>

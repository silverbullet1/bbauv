<?xml version="1.0"?>
<health-monitor>
  <monitor>
    <default init="HS_OFFLINE" timeout="5" offline="SL_ABORT" malfunc="SL_ABORT" unavail="SL_WARN"/>
    <item name="Service2 Health"/>
  </monitor>
  <poll interval="15" margin="1">
    <server addr="$Service2">
      <service type="MT_MEASUREMENT2" item="Service2 Health"/>
    </server>
  </poll>
</health-monitor>

<launch>
        <param name="robot_description" command="cat $(find fake_auv)/fake_auv.xml" />
        <node name="fake_auv" pkg="robot_state_publisher" type="state_publisher" />
        <!--node name="state_publisher" pkg="fake_auv" type="state_publisher" /-->

	<node name="manual_control" pkg="manual_control" type="manualControl" />
	<node name="joystick" pkg="joy" type="joy_node"/>
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find fake_auv)/rviz_conf.vcg" />
</launch>


# BUMBLEBEE AUV
# OpenUPS units (ordered from left to right on the USB hub)
# NOTE: change the KERNEL values if the units are connected to other physical ports
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="1-1.6.1.1", SYMLINK+="openups1", RUN="/lib/nut/usbhid-ups -a openups1 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="1-1.6.1.2", SYMLINK+="openups2", RUN="/lib/nut/usbhid-ups -a openups2 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="1-1.6.1.3", SYMLINK+="openups3", RUN="/lib/nut/usbhid-ups -a openups3 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="1-1.6.1.4", SYMLINK+="openups4", RUN="/lib/nut/usbhid-ups -a openups4 -x dev=[0]*$attr{devnum}"

# ARDUINOS
ATTRS{idVendor}=="2341", ATTRS{idProduct}=="0042", KERNEL=="ttyACM[0-9]", SYMLINK+="ttyArduino"

# DVL
ATTRS{idVendor}=="067b", ATTRS{idProduct}=="2303", KERNEL=="ttyUSB[0-9]", SYMLINK+="ttyDVL"

# AHRS
ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6001", KERNEL=="ttyUSB[0-9]", ATTRS{serial}=="FTFUUY6A", SYMLINK+="ttyAHRS"

# Microsoft Webcams
KERNEL=="video[0-9]", ATTRS{idVendor}=="045e", ATTRS{idProduct}=="075d", KERNELS=="1-1.1", SYMLINK+="videoFront"
KERNEL=="video[0-9]", ATTRS{idVendor}=="045e", ATTRS{idProduct}=="075d", KERNELS=="3-3.1.2", SYMLINK+="videoBottom"

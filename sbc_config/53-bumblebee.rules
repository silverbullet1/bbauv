# BUMBLEBEE AUV
# OpenUPS units (ordered from left to right on the USB hub)
# NOTE: change the KERNEL values if the units are connected to other physical ports
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="3-3.1.3", SYMLINK+="openups1", RUN="/lib/nut/usbhid-ups -a openups1 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="3-3.1.4", SYMLINK+="openups2", RUN="/lib/nut/usbhid-ups -a openups2 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="3-3.2", SYMLINK+="openups3", RUN="/lib/nut/usbhid-ups -a openups3 -x dev=[0]*$attr{devnum}"
ATTR{idVendor}=="04d8", ATTR{idProduct}=="d004", KERNEL=="3-3.3", SYMLINK+="openups4", RUN="/lib/nut/usbhid-ups -a openups4 -x dev=[0]*$attr{devnum}"

# ARDUINOS
# Sensors
ATTRS{idVendor}=="2341", ATTRS{idProduct}=="0042", KERNEL=="ttyACM[0-9]", ATTRS{serial}=="74133353437351C0B160", SYMLINK+="ttySensors"
# Thrusters
ATTRS{idVendor}=="2341", ATTRS{idProduct}=="0042", KERNEL=="ttyACM[0-9]", ATTRS{serial}=="7413335343735171C082", SYMLINK+="ttyThrusters"

# Microsoft Webcams
KERNEL=="video[0-9]", ATTRS{idVendor}=="045e", ATTRS{idProduct}=="075d", KERNELS=="3-3.1.1", SYMLINK+="videoFront"
KERNEL=="video[0-9]", ATTRS{idVendor}=="045e", ATTRS{idProduct}=="075d", KERNELS=="3-3.1.2", SYMLINK+="videoBottom"
